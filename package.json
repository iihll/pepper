{
  "name": "pepper",
  "version": "0.0.1",
  "description": "A Component Library for Vue.js.",
  "main": "index.js",
  "repository": "https://github.com/iihll/pepper.git",
  "author": "iihll coderlt",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config ./build/webpack.config.js",
    "build:file": "node build/bin/build-entry.js",
    "build:docs": "vuepress build docs",
    "new": "node build/bin/new.js",
    "dev": "yarn build:file && vuepress dev docs",
    "lint": "eslint .",
    "commit": "yarn prettier && git-cz",
    "commitlint": "validate-commit-msg",
    "prepare": "husky install",
    "prettier": "prettier -w ./src ./build ./packages ./test ./docs ./config",
    "test": ""
  },
  "dependencies": {
    "vue": "^2.6.12",
    "webpack": "4.0.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.41",
    "chai": "^4.3.4",
    "commitizen": "^4.2.3",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.25.0",
    "eslint-plugin-vue": "^7.9.0",
    "file-save": "^0.2.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^6.0.0",
    "json-templater": "^1.2.0",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-mocha": "^2.0.1",
    "karma-sinon-chai": "^2.0.2",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^5.0.0",
    "less": "^4.1.1",
    "less-loader": "5.0.0",
    "mocha": "^8.4.0",
    "prettier": "^2.2.1",
    "sinon": "^10.0.0",
    "sinon-chai": "^3.6.0",
    "style-loader": "^2.0.0",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^4.1.1",
    "validate-commit-msg": "^2.14.0",
    "vue-loader": "^15.9.6",
    "vue-router": "^3.5.1",
    "vue-template-compiler": "^2.6.12",
    "vuepress": "^1.8.2",
    "webpack-cli": "^4.7.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}